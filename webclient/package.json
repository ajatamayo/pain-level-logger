{
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "antd": "^3.10.0",
    "axios": "^0.18.0",
    "connected-react-router": "^4.5.0",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.0.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "name": "url-shortener",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-css": "node-sass src/ -o src/",
    "eject": "react-scripts eject",
    "start": "npm-run-all --parallel watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive"
  },
  "version": "0.1.0"
}
